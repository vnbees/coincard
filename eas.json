{
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_LEGACY_PEER_DEPS": "1"
      }
    },
    "production": {}
  },
  "cli": {
    "version": ">= 5.9.1",
    "requireCommit": false,
    "appVersionSource": "remote"
  }
}
